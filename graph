%matplotlib inline
from matplotlib import pyplot as plt 
x = [3, 6, 10]
y = [9, 12, 17]
x2 = [3, 6, 10]
y2 = [10, 13, 20]

plt.plot(x, y, color = "red")
plt.plot(x2, y2, color = "yellow")


plt.title('MY Graph')
plt.xlabel('X-AXIS')
plt.ylabel('Y-AXIS')

print("Here red line shows the Graph1 and yellow line shows the Graph 2")
plt.show()
